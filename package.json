{
  "name": "metacare-test",
  "version": "1.0.0",
  "description": "A code test from metacare",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-server",
    "local-start": "nodemon build/server.js",
    "start": "node -r esm  ./src/server.js",
    "dev": "nodemon --exec babel-node ./src/server.js",
    "lint": "eslint src/ --fix",
    "test": "node -r esm ./src/server.js && mocha --timeout 10000"
  },
  "keywords": [
    "code",
    "test"
  ],
  "author": "Lawal Samuel Olamide",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "date-fns": "^2.17.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.flatten": "^4.4.0",
    "lodash.foreach": "^4.5.0",
    "lodash.includes": "^4.3.0",
    "lodash.map": "^4.6.0",
    "lodash.orderby": "^4.6.0",
