    "mongoose": "^5.10.9",
    "nodemailer": "^6.4.14",
    "nodemailer-smtp-transport": "^2.7.4",
    "nodemon": "^2.0.5",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "winston": "^3.3.3"
